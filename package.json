{
  "name": "uni-claimer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "claim": "node dist/src/index.js",
    "build": "yarn get:proofs && yarn wrap:contracts && yarn compile:ts",
    "wrap:contracts": "node scripts/createWrappers.js",
    "get:proofs": "node scripts/fetch-proofs.js",
    "compile:ts": "rm -rf dist/ && tsc",
    "compile:ts:watch": "rm -rf dist/ && tsc --watch",
    "lint": "eslint --ext ts --fix 'src/**/*.ts' && prettier 'src/**/*.ts' --write"
  },
  "dependencies": {
    "@typechain/ethers-v5": "^1.0.0",
    "@types/chalk-animation": "^1.6.0",
    "@types/dotenv": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "axios": "^0.20.0",
    "chalk": "^4.1.0",
    "chalk-animation": "^1.6.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "ethers": "^5.0.14",
    "typechain": "^2.0.0",
    "typescript": "^4.0.2"
  }
}
